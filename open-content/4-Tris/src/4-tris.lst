5489 0080 		        TSTR    R0                      ; Is it zero?
548a 0204 0016 		        BEQ     @@sloop                 ; If so, stop -- line's not full.
			
548c 0009 		        INCR    R1
548d 0379 0018 		        CMPI    #EDGER, R1              ; At the end of the well?
548f 0226 000a 		        BLE     @@scoreloop             ; If not, keep looping
			
