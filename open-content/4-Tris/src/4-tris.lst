522c 0095 		        MOVR    R2,     R5
522d 02bc 034d 		        MVII    #REGSV, R4      ; Point to register save area
522f 02a0 		        MVI@    R4,     R0      ; Restore R0
5230 02a1 		        MVI@    R4,     R1      ; Restore R1
5231 02a2 		        MVI@    R4,     R2      ; Restore R2
