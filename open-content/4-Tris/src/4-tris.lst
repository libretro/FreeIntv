5473 01db 		        CLRR    R3                      ; Clear count of lines to score
5474 0243 01e3 		        MVO     R3,     @@full
5476 02bc 0003 		        MVII    #3,     R4              ; Loop trip count
			
5478 0200 0008 		        B       @@doscore
