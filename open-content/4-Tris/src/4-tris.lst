5432 0240 0029 		        MVO     R0,     CS1     ; Force color-stack to piece's color
5434 0240 002b 		        MVO     R0,     CS3     ; Force color-stack to piece's color
			
5436 02b9 0014 		        MVII    #EDGEL+5, R1    ; Start out in middle of well...
5438 02ba 0003 		        MVII    #3,       R2    ; ...just a little off top of screen
