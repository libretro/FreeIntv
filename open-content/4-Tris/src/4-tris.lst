5b55 02aa 		        MVI@    R5,     R2      ; Load low half of 32-bit -10**x
5b56 02ac 		        MVI@    R5,     R4      ; Load high half of 32-bit -10**x
 0x5b57                 @@divlp:
5b57 0063 		        SLR     R3,     1
 0x5b58                 @@divlpb:
5b58 000b 		        INCR    R3
5b59 00d0 		        ADDR    R2,     R0      ; Add the low half
5b5a 0029 		        ADCR    R1              ; Add carry from low half
5b5b 0053 		        RLC     R3,     1       ; See if adding the carry carried
5b5c 00e1 		        ADDR    R4,     R1      ; Add high half
