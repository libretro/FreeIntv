5b00 0275 		        PSHR    R5              ; Save return address
5b01 02bd 5adc 		        MVII    #POW10_4, R5    ; Point to '10000' entry in POW10
5b03 0244 0110 		        MVO     R4,     @@so    ; Save screen offset
5b05 0242 0111 		        MVO     R2,     @@fw    ; Save field width
5b07 02bc 0005 		        MVII    #5,     R4      ; Iterate 5 (16-bit goes to 65536)
5b09 008a 		        MOVR    R1,     R2
5b0a 000f 		        INCR    PC
