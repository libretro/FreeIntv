5413 0004 0158 02be 	        CALL    NEXTRANDX
5416 03b8 001c 		        ANDI    #$1C,   R0      ; Mask to [0..7] * 4
5418 0224 0008 		        BEQ     @@randloop      ; If it comes up 0, try again
			
541a 0338 0004 		        SUBI    #4,     R0      ; Make into piece #0..#6
			
541c 0004 0154 02ac 	        CALL    NEXTCLEAR       ; Clear previous 'next-piece'
541f 0283 01ae 		        MVI     NXTPC,  R3      ; Get previous 'next-piece'.
5421 0240 01ae 		        MVO     R0,     NXTPC   ; Save new 'next-piece'.
			
5423 0281 01ad 		        MVI     SHOWNXT, R1     ; Are we showing next piece?
5425 0241 01eb 		        MVO     R1,     DIDNXT  ; Initialize our "showed next piece" flag
5427 0089 		        TSTR    R1
5428 0204 0005 		        BEQ     @@noshow
			
