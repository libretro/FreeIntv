5bf3 0339 0002 		        SUBI    #2,     R1              ; Count down the hold count
5bf5 020b 0004 		        BMI     @@inactive              ; If count goes negative, channel is
			                                        ;    inactive.
5bf7 0204 0004 		        BEQ     @@nextrec               ; If count goes to zero, load next
			                                        ;    sound record into the channel
			
5bf9 0014 		        DECR    R4                      ; Otherwise, store the updated count.
5bfa 0261 		        MVO@    R1,     R4              ; count ==> REC[0]
			
 0x5bfb                 @@inactive:
5bfb 02b7 		        PULR    PC                      ; Return.
			
 0x5bfc                 @@backup:
5bfc 0014 		        DECR    R4
 0x5bfd                 @@nextrec:
5bfd 02a5 		        MVI@    R4,     R5              ; Load the stream record ptr (REC[1])
5bfe 02aa 		        MVI@    R5,     R2              ; Load hold tick-count from stream
