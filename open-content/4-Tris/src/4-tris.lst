5402 0273 		        PSHR    R3              ; Save R3 so that it's not clobbered
5403 0067 		        SLR     R3,     2       ; Generate index into color table
5404 02fb 5409 		        ADDI    #@@color,R3     ; R3 = PCCOLOR[piece]
5406 0298 		        MVI@    R3,     R0      ; Get the piece color into R0
5407 02b3 		        PULR    R3              ; Restore R3
5408 00af 		        JR      R5              ; Return.
			
5409 0002 0001 0003 	@@color BYTE    $2, $1, $3, $4, $5, $6, $1
540c 0004 0005 0006 0001 
