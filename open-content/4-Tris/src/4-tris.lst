547b 01e4 		        CLRR    R4
547c 0244 01e3 		        MVO     R4,     @@full
547e 0244 01e2 		        MVO     R4,     @@trip
5480 0200 000f 		        B       @@nuke
			
 0x5482                 @@doscore:
