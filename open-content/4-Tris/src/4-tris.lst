5b0b 01d2 		        CLRR    R2              ; Start with division result == 0
5b0c 02a9 		        MVI@    R5,     R1      ; Load power of 10
5b0d 000d 		        INCR    R5              ; Point to next smaller power of 10
 0x5b0e                 @@divloop:
5b0e 000a 		        INCR    R2
5b0f 0108 		        SUBR    R1,     R0      ; Divide by repeated subtraction
5b10 0221 0003 		        BC      @@divloop
5b12 00c8 		        ADDR    R1,     R0      ; Loop iterates 1 extra time: Fix it.
5b13 0012 		        DECR    R2              ; Fix extra iter.  Also test if 0
5b14 020c 0003 		        BNEQ    @@disp          ; If digit != 0, display it.
5b16 009b 		        TSTR    R3              ; If digit == 0 and no lead 0, skip
5b17 020b 000b 		        BMI     @@blank
