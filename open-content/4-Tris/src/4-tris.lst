50c1 02a9 		        MVI@    R5,     R1              ; Get two bytes
50c2 0261 		        MVO@    R1,     R4              ; Put the first byte
50c3 0041 		        SWAP    R1                      ; Put the other byte into position
50c4 0261 		        MVO@    R1,     R4              ; Put the second byte
50c5 0010 		        DECR    R0                      ; Sheesh, do I have to spell this out?
50c6 022c 0006 		        BNEQ    @@charloop              ; inner loop
50c8 0220 0013 		        B       @@gramloop              ; outer loop
			
