5ac2 0058 		        SLLC    R0,     1
5ac3 0051 		        RLC     R1,     1
5ac4 0209 0004 		        BNC     @@nocarry
			;       XORI    #$5,    R0      ; period==(2**31 - 1) polynomial
5ac6 03f9 04c1 		        XORI    #$04C1, R1      ; period==(2**32 - 1) polynomial (CRC-32 poly)
5ac8 03f8 1db7 		        XORI    #$1DB7, R0
 0x5aca                 @@nocarry:
5aca 0012 		        DECR    R2
5acb 022c 000a 		        BNEQ    @@loop
			
5acd 033c 0002 		        SUBI    #2,     R4
5acf 0260 		        MVO@    R0,     R4
5ad0 0261 		        MVO@    R1,     R4
5ad1 00af 		        JR      R5
			
			        ENDP
			
